<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView android:id="@+id/CardSchemeName"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="10dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="15dp"
    android:layout_marginEnd="10dp"
    app:cardUseCompatPadding="true"
    app:cardCornerRadius="10sp"
    app:cardBackgroundColor="@color/white"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/cw_shadow_bg"

    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="5sp"
        android:layout_marginBottom="5dp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:background="@color/white"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/iv_profile_img"
                android:layout_width="100sp"
                android:layout_height="100sp"
                android:layout_gravity="center"
                android:scaleType="fitCenter"/>

            <TextView
                android:id="@+id/tv_gender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minWidth="80sp"
                android:layout_gravity="center"
                android:gravity="center"
                android:visibility="gone"
                android:textSize="12sp"
                android:textColor="@color/colorPrimary"
                android:padding="3dp"
                />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/colorPrimary"
                android:layout_margin="5dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                >


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="5dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    >

                    <ImageView
                        android:id="@+id/iv_delete"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"

                        android:textStyle="bold"
                        android:visibility="gone"
                        android:layout_weight="1"
                        app:srcCompat="@drawable/ic_delete"
                        android:textColor="@color/black"
                        />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:layout_marginTop="5dp"
                        >



                        <TextView
                            android:id="@+id/tv_profile_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="12sp"
                            android:minWidth="120dp"
                            android:inputType="textCapSentences"
                            android:textColor="@color/white"
                            android:background="@color/primary_green"
                            android:padding="5dp"
                            />
                        <TextView
                            android:id="@+id/tv_profile_id"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:gravity="center"
                            android:textSize="12sp"
                            android:minWidth="120dp"
                            android:visibility="gone"
                            android:inputType="textCapSentences"
                            android:textColor="@color/white"
                            android:background="@color/primary_green"
                            android:padding="3dp"
                            />



                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="5dp"
                        android:weightSum="10">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"

                            android:layout_weight="5">



                            <TextView
                                android:id="@+id/tv_FatherName"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="12sp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:minLines="2"
                                android:minWidth="100sp"
                                android:textColor="@color/black"
                                android:background="@color/quantum_yellow"
                                android:padding="3dp"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Father's Name"
                                android:textSize="10sp"
                                android:layout_gravity="center"
                                android:gravity="center"

                                android:textColor="@color/gray"/>


                        </LinearLayout>
                        <View
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="@color/colorPrimary"
                            android:layout_margin="3dp"/>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="5">



                            <TextView
                                android:id="@+id/tv_MotherName"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="12sp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:minWidth="100sp"
                                android:textColor="@color/black"
                                android:minLines="2"
                                android:background="@color/quantum_yellow"
                                android:padding="3dp"
                                />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:textSize="10sp"

                                android:text="Mother's Name"
                                android:textColor="@color/gray"/>

                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/colorPrimary"
            android:layout_margin="3dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="5">


                    <TextView
                        android:id="@+id/tv_dateOfBirth"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:minWidth="80sp"
                        android:padding="3dp"

                        android:textColor="@color/colorPrimary"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:text="Date Of Birth (Age)"
                        android:textColor="@color/black"/>

                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorPrimary"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="5"
                    android:layout_marginStart="5dp">



                    <TextView
                        android:id="@+id/tv_Gotra"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="80sp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="12sp"

                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"


                        android:text="Tribe (Gotra)"
                        android:textColor="@color/black"/>


                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/colorPrimary"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="5">


                    <TextView
                        android:id="@+id/tv_FamilyOrigin"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:minWidth="80sp"
                        android:padding="3dp"

                        android:textColor="@color/colorPrimary"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:text="Family Origin"
                        android:textColor="@color/black"/>

                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorPrimary"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="5"
                    android:layout_marginStart="5dp">



                    <TextView
                        android:id="@+id/tv_ResidingAt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="80sp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="12sp"

                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"


                        android:text="Residing At"
                        android:textColor="@color/black"/>


                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/colorPrimary"
                android:layout_margin="5dp"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="6"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="3">



                    <TextView
                        android:id="@+id/tv_WorkingStatus"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="80sp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="12sp"

                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:text="Working Status"

                        android:textColor="@color/black"/>

                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorPrimary"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="3"
                    android:layout_marginStart="5dp">



                    <TextView
                        android:id="@+id/tv_WorkingAs"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="12sp"

                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"

                        android:text="Working As"
                        android:textColor="@color/black"/>


                </LinearLayout>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/colorPrimary"
                android:layout_margin="5dp"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="10"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="5">


                    <TextView
                        android:id="@+id/tv_AnnualIncome"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="80sp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="12sp"

                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"
                        android:text="Annual Income"

                        android:textColor="@color/black"/>


                </LinearLayout>
                <View
                    android:layout_width="1dp"
                    android:layout_height="match_parent"
                    android:background="@color/colorPrimary"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:layout_weight="5"
                    android:layout_marginStart="5dp">


                    <TextView
                        android:id="@+id/tv_ContactNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="80sp"
                        android:layout_gravity="center"
                        android:gravity="center"

                        android:textSize="12sp"
                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />

                    <TextView
                        android:id="@+id/tv_created_by"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="80sp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:visibility="gone"
                        android:textSize="12sp"
                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />



                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textSize="10sp"

                        android:text="Contact No."
                        android:textColor="@color/black"/>


                </LinearLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/colorPrimary"
                android:layout_margin="5dp"/>




            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center"
                android:gravity="center">



                <LinearLayout
                    android:id="@+id/LinearWhatapp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="5dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/cw_button_shadow_primary"
                    android:layout_weight="5">
                    <ImageView
                        android:layout_width="25sp"
                        android:layout_height="25sp"
                        app:srcCompat="@drawable/whatsapp"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="@color/white"
                        android:layout_marginStart="10sp"
                        android:layout_gravity="center"
                        android:text="SHARE"/>
                    <TextView
                        android:id="@+id/tv_WhatsAppNo"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:minWidth="80sp"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:visibility="gone"
                        android:textSize="12sp"
                        android:textColor="@color/colorPrimary"
                        android:padding="3dp"
                        />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/LinearCall"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="5dp"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:background="@drawable/cw_button_shadow_primary"
                    android:layout_weight="5">
                    <ImageView
                        android:layout_width="25sp"
                        android:layout_height="25sp"
                        android:tint="@color/white"
                        app:srcCompat="@drawable/ic_phone_android"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="15sp"
                        android:textColor="@color/white"
                        android:layout_marginStart="10sp"
                        android:layout_gravity="center"
                        android:text="CALL"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</LinearLayout>
</androidx.cardview.widget.CardView>